{
  "name": "murts",
  "version": "1.0.0",
  "description": "multi-user real-time simulation",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:client && npm run serve:server",
    "serve:client": "webpack serve --config-name client",
    "serve:server": "npm run build:server && npm run start:server",
    "watch:server": "webpack --config-name server --watch",
    "start:client": "node ./dist/client/bundle.js",
    "start:server": "node ./dist/server/bundle.js",
    "build:client": "webpack --config-name client",
    "build:server": "webpack --config-name server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "html-webpack-plugin": "^5.3.2",
    "ts-loader": "^9.2.5",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "node-gameloop": "^0.1.4",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "typescript": "^4.4.2"
  }
}
